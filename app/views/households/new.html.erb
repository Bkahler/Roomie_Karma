<h1> Sign Up</h1>

    <%= form_for @household do |f| %>
        <%=f.label :household_name   %>
        <%=f.text_field :username %>
        <%=f.label :email  %>
        <%=f.text_field :email %>
        <%=f.label :password  %>
        <%=f.password_field :password %>
        <%=f.label :password_confirmation %>
        <%=f.password_field :password_confirmation %>
        <%= f.submit %>
     <% end %>